{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "allowSyntheticDefaultImports": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "src",
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "Shared/Components/Client/Testimol/index.jsx",
    "Shared/Components/uploadImage/UploadImage.tsx",
    ".next/types/**/*.ts"
, "Shared/Components/Svg/arrovRight.jsx"  ],
  "exclude": [
    "node_modules"
  ],
  "paths": {
    "@/*": [
      "./*"
    ]
  }
}
